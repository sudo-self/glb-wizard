{
  "$schema": "https://raw.githubusercontent.com/cloudflare/workers-sdk/main/packages/wrangler/config-schema.json",
  "name": "glbcraft-wizardry",
  "main": "./index.html",
  "compatibility_date": "2024-01-01",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "pages_build_output_dir": "./",
  
  "vars": {
    "ENVIRONMENT": "production",
    "APP_NAME": "GlbCraft Wizardry",
    "APP_VERSION": "1.0.0"
  },
  
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1.0
  },
  
  "placement": {
    "mode": "smart"
  },
  
  "limits": {
    "cpu_ms": 50
  },
  
  "env": {
    "production": {
      "zone_id": "1d9abad06ecc114b18983949c39d4fef",
      "routes": [
        {
          "pattern": "https://glb-wizard.pages.dev",
          "custom_domain": true
        }
      ],
      "environment": "production",
      "vars": {
        "ENVIRONMENT": "production",
        "API_URL": "https://glb-wizard.jessejesse.com"
      }
    },
    
    "staging": {
      "name": "glbcraft-wizardry-staging",
      "workers_dev": true,
      "environment": "staging",
      "vars": {
        "ENVIRONMENT": "staging",
        "API_URL": "https://glb-wizard.jessejesse.com"
      }
    },
    
    "development": {
      "name": "glbcraft-wizardry-dev",
      "workers_dev": true,
      "environment": "development",
      "vars": {
        "ENVIRONMENT": "development",
        "API_URL": "http://localhost:8787"
      },
      "local": true
    }
  }
}
